{
  "connectors": {
    "sap.app": {
      "id": "ConnectorList",
      "type": "card",
      "applicationVersion": {
        "version": "1.0.0"
      }
    },
    "sap.card": {
      "type": "List",
      "header": {
        "title": "Konnektor√ºbersicht",
        "icon": {
          "src": "sap-icon://overview-chart"
        },
        "actions": [
          {
            "type": "Navigation",
            "url": "./#/management"
          }
        ]
      },
      "content": {
        "data": {
          "json": []
        },
        "item": {
          "title": "{Name}",
          "description": "{Value}",
          "icon": {"src": "{Icon}"},
          "highlight": "{State}",
          "info": {
            "value": "{Info}",
            "state": "{State}"
          }
        }
      }
    }
  }
}