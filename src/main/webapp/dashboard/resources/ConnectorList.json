{
  "sap.app": {
    "id": "ConnectorList",
    "type": "card",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.card": {
    "type": "List",
    "header": {
      "title": "Konnektor√ºbersicht",
      "icon": {
        "src": "sap-icon://overview-chart"
      },
      "actions": [
        {
          "type": "Navigation",
          "url": "./#/management",
          "target": "_self"
        }
      ]
    },
    "content": {
      "data": {
        "json": [
          {
            "Name": "Konnektoren",
            "Icon": "../images/Connector.png"
          },
          {
            "Name": "Kartenterminals",
            "Icon": "../images/CardTerminal.png"
          },
          {
            "Name": "Karten",
            "Icon": "../images/Card.png"
          }
        ]
      },
      "item": {
        "title": "{Name}",
        "description": "{Value}",
        "icon": {
          "src": "{Icon}"
        },
        "highlight": "{State}",
        "info": {
          "value": "{Info}",
          "state": "{State}"
        }
      }
    }
  }
}
