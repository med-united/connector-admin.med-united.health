{
  "cards": {
    "sap.app": {
      "id": "ConnectorList",
      "type": "card",
      "applicationVersion": {
        "version": "1.0.0"
      }
    },
    "sap.card": {
      "type": "Table",
      "header": {
        "title": "Kartenübersicht",
        "icon": {
          "src": "sap-icon://business-card"
        }
      },
      "content": {
        "data": {
          "json": [
          ]
        },
        "row": {
          "columns" : [
            {
              "title": "Kartentyp",
              "value" : "{Name}"
            },
            {
              "title": "Anzahl",
              "value": "{Value}"
            },
            {
              "title": "PIN-Status",
              "value": "{PinInfo}",
              "state": "{PinStatus}"
            },
            {
              "title": "Gültig",
              "progressIndicator" : {
                "percent": "{CertPerc}",
                "text": "{CertText}",
                "state": "{CertStatus}"
              }
            }]
        }
      }
    }
  }
}